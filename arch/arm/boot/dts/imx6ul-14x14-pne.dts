&gpmi {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_nand>;
	status = "okay";
	partition@0{
			label = "boot";
			reg = <0x0000 0x400000>; /*4M*/
	};
	partition@1{
			label = "logo";
			reg = <0x400000 0x200000>; /*2M*/
	};
	partition@2{
			label = "ENV";
			reg = <0x600000 0x100000>; /*1M*/
	};
	partition@3{
			label = "DTB";
			reg = <0x700000 0x300000>; /*1M*/
	};
	partition@4{
			label = "kernel";
			reg = <0xa00000 0x800000>; /*8M*/
	};
	partition@5{
			label = "rootfs";
			reg = <0x1200000 0xee00000>;
	};

};


